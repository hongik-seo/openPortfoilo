package course;

import java.util.Scanner;

public class CourseMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		Admin ad=new Admin();
		
		
		Scanner sc=new Scanner(System.in);
		
		
		
		
		
		while(true) {
			System.out.println("학번은");int serialNum = sc.nextInt();
			System.out.println("전공은");String major=sc.next();
			//System.out.print("학점은");int score=sc.nextInt();
			System.out.println("자바 점수는 ");int javaScore=sc.nextInt();
			System.out.println("오라클 점수는");int dataScore=sc.nextInt();
			ad.addStudentList(serialNum, major, javaScore, dataScore);
			
			ad.showStudnetList();
			
		}
		
	}

}
