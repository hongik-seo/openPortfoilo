package course;

public class Student {

	
	int serialNum;
	String major;
	int score;
	String studentPWD;
	
	
	
	Student(){
		
		this(serialNum);
	}
	
	Student(int serialNum,String major,int score,String sudentPWD){
		
		this.serialNum=serialNum;
		this.major=major;
		this.score=score;
		this.studentPWD="0000";
		
	}


	public int getSerialNum() {
		return serialNum;
	}


	public void setSerialNum(int serialNum) {
		this.serialNum = serialNum;
	}


	public String getMajor() {
		return major;
	}


	public void setMajor(String major) {
		this.major = major;
	}


	public int getScore() {
		return score;
	}


	public void setScore(int score) {
		this.score = score;
	}


	public String getStudentPWD() {
		return studentPWD;
	}


	public void setStudentPWD(String studentPWD) {
		this.studentPWD = studentPWD;
	}
	
	
	
	
	
	
	
	
}
