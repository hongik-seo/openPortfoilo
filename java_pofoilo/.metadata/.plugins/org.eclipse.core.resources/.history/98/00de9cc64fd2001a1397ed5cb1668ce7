		         return "B";
		      } else if(main.getScore() >= 70 && main.getScore() <= 79 ) {
		         return "C";
		      } else if(main.getScore() >= 60 && main.getScore() <= 69 ) {
		         return "D";
		      } else if(main.getScore() < 60) {
		         return "F";
		      }

		}


